    <section class="amenitiesSlider">
        <div class="container">
            <div class="tabsComponent customTabs">
                <ul commonTab="amenityTabs">
                    <li tabName="internal" amenitySlider="true" class="activeTab">Internal</li>
                    <li tabName="external" amenitySlider="true">External</li>
                </ul>
            </div>
        </div>
        <div class="amenityTabsHolder flexgallery">
          {# Internal Amenities #}
          <div id="internal" class="amenityTabs">
            <div class="thumbSlider">
              <div id="internalPaging" class="paging">
                <span class="currentSlide">1</span> / <span class="totalSlides"></span>
              </div>
              <div class="sliderAmenities flexslider" navFor="internalPaging">
                <ul class="slides flexImg">
                  {% for data in node.field_internal_amenities_group %}
                    <li>
                      <img src="{{ file_url(data.entity.field_amenity_image_url.value) }}" alt="{{ data.entity.field_amenity_image_al.value }}"/>
                      <p class="flex-caption">{{ data.entity.field_amenity_image_cap.value }}</p>
                    </li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            <div class="container">
              <div class="listComponent listHidden" id="list1">
                {{ node.body.value | raw }}
              </div>
              <p class="toggleBtn">
                <a href="javascript:;" toggleContent="list1">
                  <span class="more">More <span class="operators">+</span></span>
                  <span class="less">less <span class="operators">-</span></span>
                </a>
              </p>
              <p class="notes">{{ node.field_notes.value }}</p>
            </div>
          </div>

          {# External Amenities #}
          <div id="external" class="amenityTabs">
            <div class="thumbSlider">
              <div id="externalPaging" class="paging">
                <span class="currentSlide">1</span> / <span class="totalSlides"></span>
              </div>
              <div class="sliderAmenities flexslider" navFor="externalPaging">
                <ul class="slides flexImg">
                  {% for data in node.field_external_amenities_group %}
                    <li>
                      <img src="{{ file_url(data.entity.field_amenity_image_url.value) }}" alt="{{ data.entity.field_amenity_image_al.value }}"/>
                      <p class="flex-caption">{{ data.entity.field_amenity_image_cap.value }}</p>
                    </li>
                  {% endfor %}
                </ul>
              </div>
            </div>
            <div class="container">
              <div class="listComponent listHidden" id="list2">
                {{ node.body.value | raw }}
              </div>
              <p class="toggleBtn">
                <a href="javascript:;" toggleContent="list2">
                  <span class="more">More <span class="operators">+</span></span>
                  <span class="less">less <span class="operators">-</span></span>
                </a>
              </p>
              <p class="notes">*Except kitchen, passage and bathroom</p>
            </div>
          </div>
            {#{{ page.residential_amenities_slider }}#}
        </div>
    </section>
