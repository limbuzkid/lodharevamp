      <div class="formWrapper">
        <p class="heading">I WISH TO</p>
        <div class="accordianBox">
          <div class="fieldRow">
            <div class="field">
              <ul class="radioGroup">
                <li>
                  <input name="radio1" type="radio" value="" id="buy">
                  <label for="buy">Buy</label>
                </li>
                <li>
                  <input name="radio1" type="radio" value="" id="rent">
                  <label for="rent">Rent</label>
                </li>
              </ul>
            </div>
            <div class="field">
              <div class="selectWrapper">
                <div class="selectedvalue"></div>
                <select name="">
                  <option value="" selected>Type of Property</option>
                  <option>Type of Property</option>
                </select>
              </div>
            </div>
            <div class="field">
              <div class="selectWrapper">
                <div class="selectedvalue"></div>
                <select name="">
                  <option value="" selected>Select City</option>
                  <option>Mumbai</option>
                </select>
              </div>
            </div>
            <div class="field">
              <div class="selectWrapper">
                <div class="selectedvalue"></div>
                <select name="">
                  <option value="" selected>Select Locality</option>
                  <option>Parel</option>
                </select>
              </div>
            </div>
          </div>
        </div>
        <p class="heading">HOME &amp; BUDGET SIZE</p>
        <div class="accordianBox">
          <div class="fieldRow">
            <label>Select Configuration</label>
            <ul class="radioGroup">
              <li>
                <input name="radio2" type="radio" value="" id="bhk1">
                <label for="bhk1">1BHK</label>
              </li>
              <li>
                <input name="radio2" type="radio" value="" id="bhk2">
                <label for="bhk2">2BHK</label>
              </li>
              <li>
                <input name="radio2" type="radio" value="" id="bhk3">
                <label for="bhk3">3BHK</label>
              </li>
              <li>
                <input name="radio2" type="radio" value="" id="bhk4">
                <label for="bhk4">4BHK</label>
              </li>
            </ul>
          </div>
          <div class="fieldRow">
            <label>Select Budget</label>
            <div class="sliderWrap">
              <div class="slider">
                <div class="searchValslider" id="searchValslider"></div>
              </div>
              <div class="sliderRangeBox">
                <ul>
                  <li>50L</li>
                  <li>25Cr</li>
                </ul>
              </div>
            </div>
          </div>
        </div>
        <p class="heading">POSSESSION STATUS</p>
        <div class="accordianBox">
          <div class="fieldRow">
            <ul class="checkGroup">
              <li>
                <input name="checkbox" type="checkbox" value="" id="checkbox">
                <label for="checkbox">Ready Possesion</label>
              </li>
              <li>
                <input name="checkbox" type="checkbox" value="" id="checkbox2">
                <label for="checkbox2">Under Construction</label>
              </li>
              <li>
                <input name="checkbox" type="checkbox" value="" id="checkbox3">
                <label for="checkbox3">OC Ready</label>
              </li>
            </ul>
          </div>
        </div>
        <div class="buttonWrap">
          <button class="btn">Search</button>
          <button class="btn btn-black">Reset</button>
        </div>
      </div>
    </div>
  </div>
</div>

