

<body class="black">
<div class="outerWrap commercialWrapper ">
  <!-- Header starts -->
  <!-- Header starts -->
  <div class="fixedHeader">
    <a class="menu" href="javascript:;" id="menu-icon">MENU</a>
    <a class="logo" href="javascript:;"><img src="{{ file_url(directory) ~ '/' ~ node.field_comm_logo.value }}" alt="Lodha"></a>
    <div class="rightSec">
      <ul>
        <li>
          <a href="javascript:;" class="favourite"><img src="{{ file_url(directory) }}/images/icon-search.png"></a>
        </li>
        <li>
          <a href="javascript:;" class="favourite"><img src="{{ file_url(directory) }}/images/icon-favourite.png"></a>
        </li>
        <!-- <li><a href="javascript:;" class="contact"><img src="images/icon-contact.png"></a></li> -->
        <!-- <li class="dropdown"><a href="javascript:;"><img src="images/mobile-user.png"></a></li> -->
        <li class="myLodha"><a href="javascript:;">MY LODHA</a></li>
        <li class="dropdown countryLanguage"><a href="javascript:;">IND/EN</a></li>
      </ul>
    </div>
  </div>
  <!-- Header ends -->
  <!--video section starts from here-->
  <div class="containerWrap commercialSpace">

    <div class="videoBlock">
      <div class="container">
        <div class="videoWrapper">
          <div class="videoContent  owl-carousel">
            {% for banner in node.field_banner %}
              <div class="slideItem">
                {% if banner.entity.field_youtubid.0.value|length > 0 %}
                  <a class="popup-youtube" href="http://www.youtube.com/watch?v={{ banner.entity.field_youtubid.0.value }}">
                    <img src="{{file_url(banner.entity.field_image_url_text.0.value) }}" frameborder="0" alt={{ banner.entity.field_common_alternate_text.0.value }}/>
                    <span class="playBtn"></span>
                  </a>
                {% else %}
                  <a href="javascript:void(0);" id="modalBtn" style="background: url('{{ banner.entity.field_image_url_text.0.value }}') no-repeat center center"></a>
                {% endif %}
              </div>
            {% endfor %}
          </div>

          <!--<button id="modalBtn">Open Modal</button>-->
          <div class="videoText">
            <h3>{{ node.field_banner_side_text.0.entity.field_main_title.0.value }}</h3>
            <p>{{ node.field_banner_side_text.0.entity.field_sub_title.0.value }}</p>
          </div>
        </div>

      </div>
    </div>

    <!--video section ends here-->
    <!--tab section starts from here-->
    <div class="container">
      <div class="videoContentWrap">
        <div class="content">
          {{ node.body.value | raw }}
        </div>
        <div class="showMore">
          <a href="javascript:void(0);" class="moreBtn">More
            <span class="plus-icon">+</span>
          </a>
          <a href="javascript:void(0);" class="lessBtn" style="display:none;">Less
            <span class="plus-icon">-</span>
          </a>
        </div>
      </div>
    </div>
    <div class="container">
      <!--owl carousel tab menu-->
      <div class="scroller_anchor"></div>
      <div class="tabMenu" id="tabMenuBlock">
        <div class="tabCarousel  owl-carousel">
          <div class="item itemActive">
            <a href="#boutiqueOffice">Boutique Offices</a>
          </div>
          <div class="item">
            <a href="#leaseOffice">Offices for Lease</a>
          </div>
          <div class="item">
            <a href="#retail">Retail</a>
          </div>
          <div class="item">
            <a href="#warehouse">Warehouse</a>
          </div>
        </div>
      </div>

      <!--owl carousel tab menu-->
    </div>

    {{ page.categorized_commercial_project }}

    <!--download broucher section-->
    <div class="downloadBlock">
      {{ node.field_download_block_text.value | raw }}
      <div class="btnWrap">
        <a href="javascript:void(0);" class="downloadBtn">Download Brochure
          <span class="download-icon">
                                <img src="{{ file_url(directory) }}/images/download.png" alt="download" />
                            </span>
        </a>
      </div>
    </div>
    <!--download section ends here-->

  </div>
</div>

