

<body class="themeGreen plansPg amenitiesPg largeScreen">
<div class="outerWrap">
    <div class="subHeader">
        <div class="breadcrumb">
            <ul>
                <li>
                    <a href="javascript:;">Home</a>
                </li>
                <li>
                    <a href="javascript:;">Residential Properties</a>
                </li>
                <li>
                    <a href="javascript:;">Amara</a>
                </li>
                <li>About</li>
            </ul>
        </div>
        <div class="topNav">
            <ul>
                <li><a href='{{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/about') |lower  }}'>About</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/location') |lower }}>Location</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/price-plans') |lower }} >Plans & Price</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/amenities') |lower }} >Amenities</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/gallery') |lower }} class="active">Gallery</a></li>
            </ul>
            <div class="enquire-block">
                <a href="javascript:;" class="modalOpen" data-modal-id="getInTouch">
                <span class="fav-icon">
                    <img src="{{ file_url(directory) }}/images/icon-enquire.png" alt="">
                </span>
                    <span>Enquire</span>
                </a>
            </div>
        </div>
    </div>
    <!-- Header ends -->


    <section class="amenities">
        <div class="container">
            <a href="javascript:;" class="getinTouch modalOpen" data-modal-id="getInTouch">
                <img src="{{ directory }}/images//icon-enquire.png" alt="">
            </a>
            <div class="sectionTitle">
                <p>Our
                    <span>Gallery</span>
                </p>
                <div class="customTabs galTabs">
                    <ul commonTab="amenityTabs">
                        <li tabName="iframe" gallerySlider="true">360</li>
                        <li tabName="photos" gallerySlider="true">Photos</li>
                        <li tabName="videos" gallerySlider="true">Videos</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section class="gallerySlider">
        {{ page.residential_our_gallery_tabs_block }}
    </section>
    <section class="accordianWrap">
        <div class="container">
            <div class="accordian-container">
                {{ dump(node.field_gallery_accordians) |keys }}
                {% for data in node.field_gallery_accordians %}
                    <p class="heading">{{ data.entity.field_title_.value }}</p>
                    <div class="accordCont">
                        {{ data.entity.field_accordian_details.value | raw }}
                    </div>
                {% endfor %}
            </div>

        </div>
    </section>
    <section class="propertyDetails">
        <div class="container">
            <div class="wrapper">
                <p>
                    Price:
                    <strong>₹69.3 Lacs onwards</strong>
                </p>
                <p>
                    Typology:
                    <strong>2, 3 BHK</strong>
                </p>
                <p>
                    Possession Date: <strong>{{ node.field_project.0.entity.field_tentative_possession_date.value|date("M Y") }}</strong>
                </p>
                <p>
                    {#node.field_project.0.entity.field_specific_location.value ~ ', ' ~ #}
                    Location: <strong>{{ node.field_project.0.entity.field_city.0.entity.name.value }}</strong>
                </p>
            </div>
        </div>
        <div class="propertyMenu">
            <p class="fav">
                <a href="javascript:;">
                    <img src="{{ directory }}/images//icon-favourite-gold.png" alt="">
                </a>
            </p>
            <p id="share-button-2" class="need-share-button-default" data-share-icon-style="box" data-share-networks="Mailto,Twitter,Facebook,Linkedin"></p>
            <p class="download">
                <a href="javascript:;" target="_blank">
                    <img src="{{ directory }}/images//icon-download.png" alt="">
                </a>
            </p>
        </div>
    </section>
    <!-- Section residential Starts -->
    <section class="residential">
        <div class="container">
            <div class="wrapper">
                <div class="sectionTitle">
                    <p>YOU MAY
                        <span>ALSO LIKE</span>
                    </p>
                </div>
                <div id="residential" class="resComTabs">
                    <div class="resdient-slider owl-carousel">
                        {% for data in node.field_project.0.entity.field_related_projects %}
                            <div class="slides">
                                <span class="slider-tag">{{ data.entity.field_project_category.0.entity.name.value }}</span>
                                <div class="res-head flex-wrp">
                                    <div class="head-txt">
                                        {#<p>#}
                                        {#node.field_project.0.entity.field_specific_location.value ~ ', ' ~ #}
                                        {#Location: <strong>{{ node.field_project.0.entity.field_city.0.entity.name.value }}</strong>#}
                                        {#</p>#}
                                        <h3>{{ data.entity.title.value }}</h3>
                                        <p>{{ data.entity.field_specific_location.value ~ ', ' ~  data.entity.field_city.0.entity.name.value}}</p>
                                        {#<p>Ghodbunder Road, Thane</p>#}
                                    </div>
                                    <div class="rating">
                                        {#<a href="javascript:void(0);" class="viewIcon"></a>#}
                                        <!-- <span class="sprite-star"></span> -->
                                        {#<a href="javascript:void(0);"><img src="/{{ file_url(directory) }}/images/aspi/star.png"
                                        alt="star"></a>#}
                                    </div>
                                </div>
                                <div class="inner-wrp">
                                    <div class="inner-slider owl-carousel" >
                                        {% for data2 in data.entity.field_minimized_images_ %}
                                            <img src={{  file_url(data2.entity.field_image_url.value) }}  alt={{ data2.entity.field_image_alternate_text.value }} />
                                        {% endfor %}
                                    </div>
                                    <span class="fav-icon">
                                            <img src="{{ file_url(directory) }}/images/aspi/fav-icon.png" alt="fav-icon">
                                        </span>
                                </div>
                                <div class="possession-wrp">
                                    <p>Possession Date: <b>{{ data.entity.field_tentative_possession_date.value|date("M Y")}}</b></p>
                                    <p>Typology: <b>2, 3 BHK</b> | Price: <b>2.5 to 5 Cr</b></p>
                                    <p>
                                            <span>Offer :
                                                {{ data.entity.field_scheme_title.value }}</span>
                                    </p>
                                </div>
                                <div class="slide-footer">
                                    <a href="javascript:;">View Details</a>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Section residential Ends -->

