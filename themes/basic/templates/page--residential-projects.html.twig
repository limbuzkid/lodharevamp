<!-- Get in touch popup -->
<div class="modal" id="getInTouch">
    <div class="modalBody">
        <div class="close">Close</div>
        <div class="getInTouchWrap">
            <div class="getInTouchLightBox">
                <div class="container">
                    <h3>GET IN TOUCH</h3>
                    <div class="formWrapper">
                        <div class="fieldRow">
                            <div class="inputWrap"><input type="text" name="" placeholder="Name"></div>
                        </div>
                        <div class="fieldRow">
                            <div class="inputWrap"><input type="text" name="" placeholder="Email-Id"></div>
                        </div>
                        <div class="fieldRow">
                            <div class="inputWrap"><input type="text" name="" placeholder="Mobile No."></div>
                        </div>
                        <div class="fieldRow">
                            <div class="selectWrapper">
                                <div class="selectedvalue"></div>
                                <select name="">
                                    <option value="" selected>Country</option>
                                    <option>India</option>
                                </select>
                            </div>
                        </div>
                        <div class="fieldRow">
                            <div class="selectWrapper">
                                <div class="selectedvalue"></div>
                                <select name="">
                                    <option value="" selected>City</option>
                                    <option>Mumbai</option>
                                </select>
                            </div>
                        </div>
                        <div class="fieldRow">
                            <ul class="radioGroup">
                                <li><input name="radio1" type="radio" value="" id="enquire" checked><label for="enquire" >Enquire</label></li>
                                <li><input name="radio1" type="radio" value="" id="bookasitevisit"><label for="bookasitevisit">Book a site visit</label></li>
                            </ul>
                        </div>
                        <div class="fieldRow hide">
                            <div class="clm1-2">
                                <div class="inputWrap"><input type="text" name="" placeholder="dd/mm/yyyy"></div>
                            </div>
                            <div class="clm2-2">
                                <div class="inputWrap"><input type="text" name="" placeholder="Time"></div>
                            </div>
                        </div>
                        <div class="buttonWrap flex-wrp">
                            <button class="btn btn-gold">Submit</button>
                            <span class="call">Call : 022 3383 6304</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
