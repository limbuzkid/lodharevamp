                        <span class="fav-icon">
                            <img src="{{ file_url(directory) ~ '/images/icon-enquire.png' }}" alt="icon-enquire">
                        </span>
                    <span>Enquire</span></a>
            </div>
        </div>
    </div>
    <section class="townBanner homeBanner">
        <div class="scrollTab"><a href="javascript:;" class="scroll">Scroll</a></div>
        <div class="townBannerHolder">
            <div class="townSlider flexslider">
                <ul class="slides flexImg">
                    {% for data in node.field_banner_images %}
                    <li>
                        <img src="{{ file_url(data.entity.field_image_url_text.value) }}" alt="{{ data.entity.field_common_alternate_text.value }}"/>
                    </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </section>
    <section class="scheme">
        <div class="container paddingBoth">
            <div class="townTitle">{{ node.field_tagline.value }}</div>
            {{ page.scheme_header }}
        </div>
    </section>
    <section class="pillarsWrap pillar-services townshipCont">
        {{ node.body.value | raw }}
    </section>


    <section class="locatePlan">
        <div class="container paddingBoth">
            <div class="tabsComponent customTabs">
                <ul commonTab="planLocateTabs">
                    <li tabName="location" class="activeTab">Location</li>
                    <li tabName="plan">Plan</li>
                </ul>
            </div>
        </div>
        <div class="container paddingBoth">
            <div id="location" class="planLocateTabs">
                <div class="locatemap">
                    <img src="{{ file_url(node.field_plan_blueprint.value) }}" alt="BluePrint missing">
                </div>
                {{ node.field_connectivity.value | raw }}
                {#<div id="accessPop" class="accessTab">
                    <div class="connect">
                        <h2>Connectivity</h2>
                        {% for data in node.field_distances %}
                        <p><span class="tableCell">{{ data.entity.field_station_name.value }}</span>{{ data.entity.field_station_distance.value }}</p>
                        {% endfor %}
                    </div>
                    {% for data in node.field_transport_type %}
                    <div class="connect">
                        <h2>{{ data.entity.field_transport_title.value }}</h2>
                            {% for data2 in data.entity.field_content %}
                                <p class="{{ data2.entity.field_line_chooser.0.entity.field_classname.value  ~ ' ' ~  data2.entity.field_color_chooser.0.entity.name.value}}" ><span>{{ data2.entity.field_text_content.value }}</span>
                                </p>
                            {% endfor %}
                    </div>
                    {% endfor %}
                    <a href="javascript:;" class="closeAccessPop closeBtn"><img src="{{ file_url(directory) }}/images/close-access.gif" alt=""></a>
                </div>#}
                <div class="locateAdd">
                    <div class="address">{{ node.field_one_liner_address.value }}</div>
                    <div class="access">
                        <a href="javascript:;" class="openAccess"><img src="{{ file_url(directory) }}/images/access.png" valign="middle"> See Access</a>
                        <a href="javascript:;" class="closeAccessPop gotomap"><img src="{{ file_url(directory) }}/images/map-icon.png" valign="middle"> Go To Map</a>
                    </div>
                </div>
            </div>
            <div id="plan" class="planLocateTabs">
                <div class="explore">
                    <p>Tap to see a detailed view of THE TOWNSHIP</p>
                    <p><img src="{{ file_url(directory) }}/images/360.png" alt=""></p>
                    <p><a href="javascript:;" class="btn btn-white exploreIframe" rel="iframe-360.html">Explore</a></p>
                </div>
                <div id="exploreIframe">
                    <iframe src="iframe-360.html" frameborder="0"></iframe>
                </div>
            </div>
        </div>
    </section>
