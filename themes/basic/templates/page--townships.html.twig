    <!-- Header starts -->
    <div class="fixedHeader">
        <a class="menu" href="javascript:;" id="menu-icon">MENU</a>
        <a class="logo" href="javascript:;"><img src="{{ file_url(node.field_township_logo.value) }}" alt="New Cuffe Parade"></a>
