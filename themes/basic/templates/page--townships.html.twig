    <section class="propertyDetails">
        <div class="propertyMenu">
            <p class="fav"><a href="javascript:;"><img src="{{ file_url(directory) ~ '/images/icon-favourite-gold.png' }}" alt=""></a></p>
            <p id="share-button-2" class="need-share-button-default" data-share-icon-style="box" data-share-networks="Mailto,Twitter,Facebook,Linkedin"></p>
            <p class="download"><a href="javascript:;" target="_blank"><img src="{{ file_url(directory) ~ '/images/icon-download.png' }}" alt=""></a></p>
        </div>
    </section>
    <!-- Section residential Starts -->
    <section class="residential">
        <div class="container">
            <div class="wrapper">
                <div class="tabsComponent customTabs">
                    <ul commonTab="resComTabs">
                        <li tabName="residential" class="activeTab">Residential</li>
                        <li tabName="commercial">Commercial</li>
                    </ul>
                </div>
                <!--Carousel Residential Starts -->
                <div id="residential" class="resComTabs">
                    <div class="resdient-slider owl-carousel">
                        {% for data in node.field_related_projects %}
                            {% if data.entity.field_project_category.0.entity.name.value|lower == 'residential' %}
                            <div class="slides">
                                <span class="slider-tag">{{ data.entity.field_project_category.0.entity.name.value }}</span>
                                <div class="res-head flex-wrp">
                                    <div class="head-txt">
                                        <h3>{{ data.entity.title.value }}</h3>
                                        <p>{{ data.entity.field_specific_location.value ~ ', ' ~  data.entity.field_city.0.entity.name.value}}</p>
                                        {#<p>Ghodbunder Road, Thane</p>#}
                                    </div>
                                    <div class="rating">
                                        {#<a href="javascript:void(0);" class="viewIcon"></a>#}
                                        <!-- <span class="sprite-star"></span> -->
                                        <a href="javascript:void(0);"><img src="{{ file_url(directory) }}/images/aspi/star.png"
                                        alt="star"></a>
                                    </div>
                                </div>
                                <div class="inner-wrp">
                                    <div class="inner-slider owl-carousel" >
                                        {% for data2 in data.entity.field_minimized_images_ %}
                                            <img src={{  file_url(data2.entity.field_image_url.value) }}  alt={{ data2.entity.field_image_alternate_text.value }} />
                                        {% endfor %}
                                    </div>
                                    <span class="fav-icon">
                                            <img src="{{ file_url(directory) }}/images/aspi/fav-icon.png" alt="fav-icon">
                                        </span>
                                </div>
                                <div class="possession-wrp">
                                    <p>Possession Date: <b></b></p>
                                    <p>Typology: <b>2, 3 BHK</b> | Price: <b>2.5 to 5 Cr</b></p>
                                    <p>
                                            <span>Offer :
                                                {{ data.entity.field_scheme_title.value }}</span>
                                    </p>
                                </div>
                                <div class="slide-footer">
                                    <a href="javascript:;">View Details</a>
                                </div>
                            </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>
                <!-- Carousel Residential Ends -->
                <!--Carousel Commercial Starts -->
                <div id="commercial" class="resComTabs" style="display:none;">
                    <div class="resdient-slider owl-carousel">
                        {% for data in node.field_related_projects %}
                            {% if data.entity.field_project_category.0.entity.name.value|lower == 'commercial' %}
                            <div class="slides">
                                <span class="slider-tag">{{ data.entity.field_project_category.0.entity.name.value }}</span>
                                <div class="res-head flex-wrp">
                                    <div class="head-txt">
                                        <h3>{{ data.entity.title.value }}</h3>
                                        <p>{{ data.entity.field_specific_location.value ~ ', ' ~  data.entity.field_city.0.entity.name.value}}</p>
                                        {#<p>Ghodbunder Road, Thane</p>#}
                                    </div>
                                    <div class="rating">
                                        {#<a href="javascript:void(0);" class="viewIcon"></a>#}
                                        <!-- <span class="sprite-star"></span> -->
                                        <a href="javascript:void(0);"><img src="{{ file_url(directory) }}/images/aspi/star.png" alt="star"></a>
                                    </div>
                                </div>
                                <div class="inner-wrp">
                                    <div class="inner-slider owl-carousel" >
                                        {% for data2 in data.entity.field_minimized_images_ %}
                                            <img src={{  file_url(data2.entity.field_image_url.value) }}  alt={{ data2.entity.field_image_alternate_text.value }} />
                                        {% endfor %}
                                    </div>
                                    <span class="fav-icon">
                                            <img src="{{ file_url(directory) }}/images/aspi/fav-icon.png" alt="fav-icon">
                                        </span>
                                </div>
                                <div class="possession-wrp">
                                    <p>Possession Date: <b>{% if data.entity.field_tentative_possession_date.value|length > 0 %}{{ data.entity.field_tentative_possession_date.value|date("M Y")}}{% else %}{{ "OC as well as under construcion" }}{% endif %}</b></p>
                                    <p>Typology: <b>2, 3 BHK</b> | Price: <b>2.5 to 5 Cr</b></p>
                                    <p>
                                            <span>Offer :
                                                {{ data.entity.field_scheme_title.value }}</span>
                                    </p>
                                </div>
                                <div class="slide-footer">
                                    <a href="javascript:;">View Details</a>
                                </div>
                            </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>
                <!-- Carousel Commercial Ends -->
            </div>
        </div>
    </section>
    <!-- Section residential Ends -->

</div>
