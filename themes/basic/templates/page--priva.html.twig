<body class="priva">
<div class="outerWrap priva largescreen">
    <!-- Header starts -->
    <div class="fixedHeader">
        <a class="menu" href="javascript:;" id="menu-icon">MENU</a>
        <a class="logo" href="javascript:;">
            <img src="{{ file_url(node.field_lodha_logo.value) }}" alt="Lodha"></a>
        <div class="rightSec">
            <ul>
                <li>
                    <a href="javascript:;" class="favourite"><img src="{{ file_url(directory) }}/images/icon-search.png"></a>
                </li>
                <li>
                    <a href="javascript:;" class="favourite"><img src="{{ file_url(directory) }}/images/icon-favourite.png"></a>
                </li>
                <!-- <li><a href="javascript:;" class="contact"><img src="{{ file_url(directory) }}/images/icon-contact.png"></a></li> -->
                <!-- <li class="dropdown"><a href="javascript:;"><img src="{{ file_url(directory) }}/images/mobile-user.png"></a></li> -->
                <li class="myLodha"><a href="javascript:;">MY LODHA</a></li>
                <li class="dropdown countryLanguage"><a href="javascript:;">IND/EN</a></li>
            </ul>
        </div>
    </div>
    <!-- Header ends -->
    <!-- topBanner starts-->
    <div class="privaBannerwrapper">
        <div class="yellowBg">
        </div>

        <div class="bannerImage" style="background-image: url({{ file_url(node.field_banner_image_.value) }}) no-repeat scroll center center;">
            <div class="bannerLogo">
                <img src="{{ file_url(node.field_priva_logo.value) }}" alt="priva-logo" />
            </div>
            <div class="bannerText">
                {{ node.field_banner_text.value | raw}}
            </div>

        </div>
        <div class="content">
            {{ node.field_banner_content.value | raw }}
        </div>
    </div>
    <!-- earnPoints start-->
    <section class="earnPoints">
        <div class="topSection">
            <div class="container paddingBoth">
                <div class="sectionTitle">
                        <span class="icon">
                            <img src="{{ file_url(directory) }}/images/priva/earn-points.png">
                        </span>
                    <p>Earn
                        <span>Points</span>
                    </p>
                </div>
                <div class="content">
                    {{ dump(node.field_earn)|keys }}
                    <p>{{ node.field_earn_point_title.value | raw }}</p>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="container">
            <div class="wrapper">
                <div class="earnPointsSlider owl-carousel">
                    {#<div>
                    <img src="{{ directory }}/images/priva/topband.png" alt="">
                    <div class="slides-box box-1">
                        <div>
                            <h3>1.2 Lac</h3>
                            <span>Silver</span>
                        </div>
                    </div>
                </div>
                    <div>
                        <img src="{{ directory }}/images/priva/topband.png" alt="">
                        <div class="slides-box box-2">
                            <div>
                                <h3>1.4 Lac</h3>
                                <span>Gold</span>
                            </div>
                        </div>
                    </div>
                    <div>
                        <img src="{{ directory }}/images/priva/topband.png" alt="">
                        <div class="slides-box box-3">
                            <div>
                                <h3>1.6 Lac</h3>
                                <span>Platinum</span>
                            </div>
                        </div>
                    </div>#}
                    {%  for data in node.field_earn_points_slider %}
                        <div>
                            <img src="{{ file_url(data.entity.field_amenity_image_url.value) }}">
                            {{ data.entity.field_amenity_description.value | raw }}
                        </div>
                    {% endfor %}
                </div>
              {{ node.field_earn_point_rewards_section.value |raw}}
            </div>
        </div>
    </section>
    <!-- earnPoints ends-->

    <!-- redeemPoints start-->
    <section class="redeemPoints">
        <div class="topSection">
            <div class="container paddingBoth">
                <div class="sectionTitle">
                        <span class="icon">
                            <img src="{{ file_url(directory) }}/images/priva/redeem-points.png">
                        </span>
                    <p>Redeem
                        <span>Points</span>
                    </p>
                </div>
                <div class="content">
                    <p>{{ node.field_ree.value | raw}}</p>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="container">
            <div class="wrapper">
                <div class="redeemPointsSlider owl-carousel">
                    {% for data in node.field_reedem_points_content.0.entity.field_group %}
                        <div class="slides-box">
                            <div>
                                <img src="{{ file_url(data.entity.field_amenity_image_url.value) }}" alt="{{ data.entity.field_amenity_image_al.value }}"/>
                                {{ data.entity.field_amenity_description.value | raw }}
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>
    <!-- redeemPoints ends-->

    <!-- whyPriva start-->
    <section class="whyPriva">
        <div class="topSection">
            <div class="container paddingBoth">
                <div class="sectionTitle">
                        <span class="icon">
                            <img src="{{ file_url(directory) }}/images/priva/why-priva.png">
                        </span>
                    <p>Why
                        <span>Priva</span>
                    </p>
                </div>
                <div class="content">
                    <p>{{ node.field_why_priva_.value | raw }}</p>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="container paddingBoth">
            <div class="wrapper">
                {% for data in node.field_why_priva_group_section.0.entity.field_group %}
                    <div class="list-box">
                        <div>
                            <h3 class="item-title">
                                {{ data.entity.field_image_title.value |raw }}
                                <span>
                                      {{ data.entity.field_image_subtitle_.value |raw }}
                                </span>
                            </h3>
                            <img src="{{file_url(data.entity.field_amenity_image_url.value) }}">
                            <div class="content">
                                {{ data.entity.field_amenity_description.value | raw }}
                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
    <!-- whyPriva ends-->

    <!-- highlights start-->
    <section class="highlights">
        <div class="topSection">
            <div class="container paddingBoth">
                <div class="sectionTitle">
                        <span class="icon">
                            <img src="{{ file_url(directory) }}/images/priva/highlights.png">
                        </span>
                    <p>Highlights</p>
                </div>
            </div>
        </div>
        <div class="container paddingBoth">
            <div class="wrapper">
                {% for data in node.field_highlights %}
                    <div class="members-enrolled">
                        <h3>{{ data.entity.field_main_title.value | raw }}</h3>
                        <span>{{ data.entity.field_sub_title.value | raw}}</span>
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
    <!-- highlights ends-->

    <!-- our giveways start-->
    <section class="propertyPrice overGiveaways">
        <div class="container paddingBoth">
            <div class="sectionTitle active">
                    <span class="icon">
                        <img src="{{ directory }}/images/priva/whitegiftbox.png">
                    </span>
                <p>Our <span>Giveaways</span></p>
                <span class="before-block"></span>
            </div>
            <div class="ourgivewayscarousel carousel owl-carousel">
                {% for data in node.field_our_giveaways %}
                    <div class="slide">
                        <div class="box">
                            <img src="{{ file_url(data.entity.field_about_image_url.value) }}" alt="{{ data.entity.field_about_image_alternate_.value }}">
                        </div>
                        {{ data.entity.field_about_image_description.value | raw}}
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
    <!-- our giveways end-->

    <!-- our testimonials-->
    <section class="testimonials">
        <div class="topSection">
            <div class="container paddingBoth">
                <div class="sectionTitle">
                        <span class="icon">
                            <img src="{{ file_url(directory) }}/images/priva/testimonials.png">
                        </span>
                    <p>Testimonials</p>
                </div>
            </div>
        </div>
        <div class="container paddingBoth">
            <div class="wrapper">
                <div class="testimonials-slider owl-carousel">
                    {% for data in node.field_testimonials %}
                    <div class="slides">
                        <img src="{{ file_url(data.entity.field_image_path_url.value) }}" alt="{{ data.entity.field_image_alter_text.value }}">
                        <div class="content">
                            {{ data.entity.field_content_body.value | raw}}
                        </div>

                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>
    <!-- our testimonials-->

    <!-- prime member -->
    <section class="priva-member">
        <div class="container">
            <div class="wrapper">
                <h4>Enjoy exclusive privileges of being a Lodha Priva member</h4>
                <button class="btn btn-gold">Login</button>
                <div class="queries">
                    <span>For any queries</span>
                    <span><b>Call</b> {{ node.field_contact.value | raw}} | <b>Email</b> {{ node.field_email.value |raw}}</span>
                </div>
            </div>
        </div>
    </section>
    <!-- prime member -->

