    <section class="amenities">
        <div class="container">
            <div class="sectionTitle">
                <p>Our
                    <span>Gallery</span>
                </p>
                <div class="customTabs galTabs">
                    <ul commonTab="amenityTabs">
                        <li tabName="iframe" gallerySlider="true"
                            style="background-image: url({{ file_url(directory) }}/images/gallery-icons.png); background-repeat: no-repeat;">360</li>
                        <li tabName="photos" gallerySlider="true"
                            style="background-image: url({{ file_url(directory) }}/images/gallery-icons.png); background-repeat: no-repeat;"
                        >Photos</li>
                        <li tabName="videos" gallerySlider="true"
                            style="background-image: url({{ file_url(directory) }}/images/gallery-icons.png); background-repeat: no-repeat;">Videos</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    {{ node.field_gallery_360_group | keys }}
    <section class="gallerySlider">
        {#{{ page.residential_our_gallery_tabs_block }}#}
        <div class="amenityTabsHolder flexgallery">
            <div id="iframe" class="amenityTabs">
                <div class="thumbSlider">
                    <div id="360Paging" class="paging">
                        <span class="currentSlide">1</span> /
                        <span class="totalSlides"></span>
                    </div>
                    <div id="360Slider" class="sliderGallery flexslider" navFor="360Paging" syncfor="360Thumb">
                        <ul class="slides flexImg">
                            {% for gallery_img in node.field_gallery_360_group %}
                                <li>
                                    <iframe style="background: linear-gradient(to right, #ffe288, #fce6b5);" frameborder="0" content=""></iframe>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                    <div id="360Thumb" syncTo="360Slider" class="galleryThumb flexslider thumbComp">
                        <ul class="slides">
                            {% for gallery_img in node.field_gallery_360_group %}
                                <li>
                                    <img src="{{file_url(gallery_img.entity.field_thumbnail.value) }}" frameborder="0"/>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
            </div>
            <div id="photos" class="amenityTabs">
                <div class="thumbSlider">
                    <div id="photosPaging" class="paging">
                        <span class="currentSlide">1</span> /
                        <span class="totalSlides"></span>
                    </div>
                    <div id="photoSlider" class="sliderGallery flexslider" navFor="photosPaging" syncfor="photosThumb">
                        <ul class="slides flexImg">
                            {% for gallery_img in node.field_gallery_ima %}
                                <li>
                                    <img src="{{file_url(gallery_img.entity.field_image_url.value) }}" alt={{ gallery_img.entity.field_caption.value }}/>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                    <div id="photosThumb" syncTo="photoSlider" class="galleryThumb flexslider thumbComp">
                        <ul class="slides">
                            {% for gallery_img in node.field_gallery_ima %}
                                <li>
                                    <img src="{{file_url(gallery_img.entity.field_image_url.value) }}" frameborder="0"/>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
            </div>
            <div id="videos" class="amenityTabs">
                <div class="thumbSlider">
                    <div id="videosPaging" class="paging">
                        <span class="currentSlide">1</span> /
                        <span class="totalSlides"></span>
                    </div>
                    <div id="videoSlider" class="sliderGallery flexslider" navFor="videosPaging" syncfor="videosThumb">
                        <ul class="slides flexImg">
                            {#{% for data in content["#data_obj"] %}
                                <li>
                                    <a class="popup-youtube" href="http://www.youtube.com/watch?v=0O2aH4XLbto">
                                        <img src="{{ file_url(directory) }}/images//floor-plan.jpg" />
                                    </a>
                                </li>
                            {% endfor %}#}
                            {% for gallery_img in node.field_gallery_videos_group %}
                                <li>
                                    <a class="popup-youtube" href="http://www.youtube.com/watch?v={{gallery_img.entity.field_youtube_id.value}}">
                                        <img src="{{file_url(gallery_img.entity.field_thumbnail_url.value) }}" frameborder="0" alt={{ gallery_img.entity.field_caption.value }}/>
                                    </a>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                    <div id="videosThumb" syncTo="videoSlider" class="galleryThumb flexslider thumbComp">
                        <ul class="slides">
                            {% for gallery_img in node.field_gallery_videos_group %}
                                <li>
                                    <img src="{{file_url(gallery_img.entity.field_thumbnail_url.value) }}" frameborder="0" alt={{gallery_img.entity.field_caption.value  }}/>
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <section class="accordianWrap">
        <div class="container">
            <div class="accordian-container">
                {% for data in node.field_gallery_accordians %}
                    <p class="heading">{{ data.entity.field_title_.value }}</p>
                    <div class="accordCont">
                        {{ data.entity.field_accordian_details.value | raw }}
                    </div>
                {% endfor %}
            </div>

        </div>
    </section>
    <section class="propertyDetails">
        <div class="container">
            <div class="wrapper">
                <p>
                    Price:
                    <strong>â‚¹69.3 Lacs onwards</strong>
                </p>
                <p>
                    Typology:
                    <strong>2, 3 BHK</strong>
                </p>
                <p>
                    Possession Date: <strong>{{ node.field_project.0.entity.field_tentative_possession_date.value|date("M Y") }}</strong>
                </p>
                <p>
                    {#node.field_project.0.entity.field_specific_location.value ~ ', ' ~ #}
                    Location: <strong>{{ node.field_project.0.entity.field_city.0.entity.name.value }}</strong>
                </p>
            </div>
        </div>
        <div class="propertyMenu">
            <p class="fav">
                <a href="javascript:;">
                    <img src="{{ file_url(directory) }}/images/icon-favourite-gold.png" alt="">
                </a>
            </p>
            <p id="share-button-2" class="need-share-button-default" data-share-icon-style="box" data-share-networks="Mailto,Twitter,Facebook,Linkedin"></p>
            <p class="download">
                <a href="javascript:;" target="_blank">
                    <img src="{{ file_url(directory) }}/images/icon-download.png" alt="">
                </a>
            </p>
        </div>
    </section>
    <!-- Section residential Starts -->
    <section class="residential">
        <div class="container">
            <div class="wrapper">
                <div class="sectionTitle">
                    <p>YOU MAY
                        <span>ALSO LIKE</span>
                    </p>
                </div>
                <div id="residential" class="resComTabs">
                    <div class="resdient-slider owl-carousel">
                        {% for data in node.field_project.0.entity.field_related_projects %}
                            <div class="slides">
                                <span class="slider-tag">{{ data.entity.field_project_category.0.entity.name.value }}</span>
                                <div class="res-head flex-wrp">
                                    <div class="head-txt">
                                        {#<p>#}
                                        {#node.field_project.0.entity.field_specific_location.value ~ ', ' ~ #}
                                        {#Location: <strong>{{ node.field_project.0.entity.field_city.0.entity.name.value }}</strong>#}
                                        {#</p>#}
                                        <h3>{{ data.entity.title.value }}</h3>
                                        <p>{{ data.entity.field_specific_location.value ~ ', ' ~  data.entity.field_city.0.entity.name.value}}</p>
                                        {#<p>Ghodbunder Road, Thane</p>#}
                                    </div>
                                    <div class="rating">
                                        {#<a href="javascript:void(0);" class="viewIcon"></a>#}
                                        <!-- <span class="sprite-star"></span> -->
                                        {#<a href="javascript:void(0);"><img src="/{{ file_url(directory) }}/images/aspi/star.png"
                                        alt="star"></a>#}
                                    </div>
                                </div>
                                <div class="inner-wrp">
                                    <div class="inner-slider owl-carousel" >
                                        {% for data2 in data.entity.field_minimized_images_ %}
                                            <img src={{  file_url(data2.entity.field_image_url.value) }}  alt={{ data2.entity.field_image_alternate_text.value }} />
                                        {% endfor %}
                                    </div>
                                    <span class="fav-icon">
                                            <img src="{{ file_url(directory) }}/images/aspi/fav-icon.png" alt="fav-icon">
                                        </span>
                                </div>
                                <div class="possession-wrp">
                                    <p>Possession Date: <b>{{ data.entity.field_tentative_possession_date.value|date("M Y")}}</b></p>
                                    <p>Typology: <b>2, 3 BHK</b> | Price: <b>2.5 to 5 Cr</b></p>
                                    <p>
                                            <span>Offer :
                                                {{ data.entity.field_scheme_title.value }}</span>
                                    </p>
                                </div>
                                <div class="slide-footer">
                                    <a href="javascript:;">View Details</a>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Section residential Ends -->



</div>

