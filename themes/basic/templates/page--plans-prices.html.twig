<body class="themeGreen plansPg largeScreen tabFixed" onload="reset()">
<div class="outerWrap">
    {% set path_url = path('entity.node.canonical', {'node': node.id})|split('/',-1)|join('/') %}
    <div class="fixedHeader">
        <a class="menu" href="javascript:;" id="menu-icon">MENU</a>
        <a class="logo" href="javascript:;">
            <img src="{{ file_url(node.field_project.0.entity.field_image_path.value) }}"
                 alt="{{ node.field_project.0.entity.title.value }}">
        </a>
        <div class="rightSec">
            <ul>
                <li><a href="javascript:;" class="favourite"><img
                                src="{{ file_url(directory) }}/images/icon-search.png"></a></li>
                <li><a href="javascript:;" class="favourite"><img
                                src="{{ file_url(directory) }}/images/icon-favourite.png"></a></li>
                <!-- <li><a href="javascript:;" class="contact"><img src="{{ directory }}/images//icon-contact.png"></a></li> -->
                <!-- <li class="dropdown"><a href="javascript:;"><img src="{{ directory }}/images//mobile-user.png"></a></li> -->
                <li class="myLodha"><a href="javascript:;">MY LODHA</a></li>
                <li class="dropdown countryLanguage"><a href="javascript:;">IND/EN</a></li>
            </ul>
        </div>
    </div>

    <div class="subHeader">
        <div class="breadcrumb">
            <ul>
                <li><a href="javascript:;">Home</a></li>
                <li><a href="javascript:;">Residential Properties</a></li>
                <li><a href="javascript:;">Amara</a></li>
                <li>About</li>
            </ul>
        </div>
        <div class="topNav">
            <ul>
               {# <li><a href='{{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/about') |lower  }}'>About</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/location') |lower }}>Location</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/price-plans') |lower }} class="active">Plans & Price</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/amenities') |lower }} >Amenities</a></li>
                <li><a href={{ ('/projects/' ~ node.field_project.0.entity.field_project_category.0.entity.name.value ~ '/' ~ node.field_project.0.entity.title.value ~ '/gallery') |lower }}>Gallery</a></li>#}
                <li><a href={{path_url ~ '/about' }} >About</a></li>
                <li><a href={{path_url ~ '/location' }}>Location</a></li>
                <li><a href={{path_url ~ '/plans-prices' }} class="active">Plans & Price</a></li>
                <li><a href={{path_url ~ '/amenities' }}>Amenities</a></li>
                <li><a href={{path_url ~ '/gallery' }}>Gallery</a></li>
            </ul>
            <div class="enquire-block">
                <a href="javascript:;" class="modalOpen" data-modal-id="getInTouch">
                <span class="fav-icon">
                    <img src="{{ file_url(directory) }}/images/icon-enquire.png" alt="">
                </span>
                    <span>Enquire</span>
                </a>
            </div>
        </div>
    </div>
    <!-- Header ends -->
    <section class="propertyPrice">
        <!-- <div class="sectionTitle">
                    <span class="icon"><img src="/{{ directory }}/images/icon-plan-prices.png"></span>
                    <p>Plans &amp; <span>Prices</span></p>
                </div> -->
        <div class="container">
            {#<a href="javascript:;" class="getinTouch modalOpen" data-modal-id="getInTouch"><img src="/{{ directory }}/images/icon-enquire.png" alt=""></a>#}
            <div class="sectionTitle active">
                <span class="icon"><img src="/{{ directory }}/images/icon-plan-prices.png"></span>
                <p>Plans &amp; <span>Prices</span></p>
                <span class="before-block"></span>
            </div>
            {#{{ page.plans_prices_header }}#}
            <div class="planTypes">
                <div class="planTabsUl customTabs">
                    <ul commonTab="planTabs">
                        <li tabName="masterPlan" class="activeTab">Master Plan</li>
                        <li tabName="floorPlan" checkSlider="true">Floor Plan</li>
                        <li tabName="unitPlan">Unit Plan</li>
                    </ul>
                </div>
                <div class="planTabContent">
                    <div id="masterPlan" class="planTabs">
                        <div><img src={{ file_url(node.field_master_plan.0.entity.field_plan_images.value) }}></div>
                        <div class="priceContent content">
                            <div class="richTxt">
                                {{ node.field_master_plan.0.entity.field_description.value|raw }}
                            </div>
                        </div>
                    </div>
                    <div id="floorPlan" class="planTabs">
                        <div class="thumbSlider">
                            <div id="floorPaging" class="paging">
                                <span class="currentSlide">1</span> /
                                <span class="totalSlides"></span>
                            </div>
                            <div id="sliderFloor" class="flexslider" navFor="floorPaging">
                                <ul class="slides">
                                    {% for data in node.field_floor_plan %}
                                        <li>
                                            <img src="{{ file_url(data.entity.field_floor_plan_images.value) }}"/>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </div>
                            <div id="carouselFloor" class="flexslider thumbComp">
                                <ul class="slides">
                                    {% for data in node.field_floor_plan %}
                                        <li>
                                            <img src="{{ file_url(data.entity.field_floor_plan_images.value) }}"/>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </div>
                            <div class="priceContent content">
                                <div class="richTxt">
                                    <p>{{ node.field_floor_plan_description.value|raw }}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="unitPlan" class="planTabs">
                        <p class="selectBhkText">Select unit here to see plans & price</p>
                        <div class="planTabsInner bhkdrop" style="/* border: 1px solid #000; */">
                            <div class="selectPlan">
                                <span class="prevPlan" onclick="minusOneRoom()">Previous</span>
                                <span class="nextPlan" onclick="plusOneRoom()">Next</span>
                                <div id="test"></div>
                                <div class="planUnitHolder" style="width: 302px;top: 0;left: 23%;">
                                    <div class="planUnits" style="width: 232px;height: 50px;">
                                        <span style="font-size: 2.5rem;text-align: left;padding: 7px 12px;"
                                              id="typology">{% if node.field_unit_plan.0.entity.field_typology.0.entity.name.value > 5 %}{{ "VILLA" }}{% else %}{{ node.field_unit_plan.0.entity.field_typology.0.entity.name.value ~ " BHK" }}{% endif %}</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="newDropdown" style="">
                            <div class="propertyPlans" style="margin-top: 0;">
                                <div class="typedropdown">
                                    <select name="" id="ptype" onchange="changePlan()">
                                        {% for data in node.field_unit_plan %}
                                            {% if node.field_unit_plan.0.entity.field_typology.0.entity.name.value == data.entity.field_typology.0.entity.name.value %}
                                                <option value="{{ data.entity.field_room_type_name.0.entity.name.value }}">{{ data.entity.field_room_type_name.0.entity.name.value }}</option>
                                            {% endif %}
                                        {% endfor %}
                                        {#<option value="images/plan.jpg|images/plan.jpg|₹69.3 Lacs onwards|Oct 2019">WEST WING</option>
                                        <option value="images/plan.jpg|images/plan.jpg|₹69.3 Lacs onwards|Oct 2021">EAST WING</option>
                                        <option value="images/plan.jpg|images/plan.jpg|₹69.3 Lacs onwards|Oct 2033">SOUTH WING</option>
                                        <option value="images/plan.jpg|images/plan.jpg|₹69.3 Lacs onwards|Oct 2045">NORTH WING</option>#}
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="propertyTypeHolder">
                            {#<div class="propertyNav owl-carousel">
                                <div class="item">
                                    <span class="active">2 Bed Optima</span>
                                </div>
                                <div class="item">
                                    <span>2 Bed Ultima</span>
                                </div>
                                <div class="item">
                                    <span>2 Bed Optima</span>
                                </div>
                                <div class="item">
                                    <span>2 Bed Ultima</span>
                                </div>
                            </div>#}
                            <div class="planCarousel zoom-gallery">
                                <div class="unitplanCarousel owl-carousel">
                                    <div class="item">
                                        <a class="zoom"
                                           href="{{ file_url(node.field_unit_plan.0.entity.field_unit_plan_images.value) }}"
                                           id="planImage1">
                                            <img src="{{ file_url(node.field_unit_plan.0.entity.field_unit_plan_images.value) }}"
                                                 id="planImage2">
                                            <span>Zoom</span>
                                        </a>
                                        <div class="priceContent">
                                            <p>Price :
                                                <span id="price">{{ node.field_unit_plan.0.entity.field_unit_price.value / 100000 ~ ' Lacs' }}</span>
                                                <span class="divider">|</span> Savings :
                                                <span id="saving">₹{{ (node.field_unit_plan.0.entity.field_unit_price.value - node.field_unit_plan.0.entity.field_specialprice.value)/ 100000 ~ ' Lacs' }}</span>
                                            </p>
                                            <p>
                                                <span class="special"
                                                      id="specialPrice">Special Price: ₹{{ node.field_unit_plan.0.entity.field_specialprice.value / 100000 ~ ' Lacs' }}</span>
                                            </p>
                                            <p>Possession:
                                                <span id="possetion" >{% if node.field_project.0.entity.field_tentative_possession_date.value %}{{ node.field_project.0.entity.field_tentative_possession_date.value|date("M Y") }}{% else %}OC as well as under construction{% endif %}</span>
                                            </p>
                                            {#<p>
                                                <a href="#">View all prices</a>
                                            </p>#}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="scheme plansWrap">
        <div class="container paddingBoth">
            <div class="wrapper flex-wrp">
                <div class="scheme-logo">
