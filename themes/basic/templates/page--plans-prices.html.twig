<script>
    var mL = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
    window.minusOneRoom = function () {
        if (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) - 1 < 5) {
            if (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) - 1 > 0) {
                document.getElementById('typology').innerText = (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) - 1) + ' BHK';
            }
        } else {
            if (document.getElementById('typology').innerText === "VILLA") {
                document.getElementById('typology').innerText = '5 BHK';
            } else {
                document.getElementById('typology').innerText = /*(parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g,''))+1) + */' VILLA';
            }
        }
        let url = window.location.href;
        ajaxQueirer('/ajax/load-unit-plan/' + url.substring(url.lastIndexOf('/', url.lastIndexOf('/') - 1) + 1, url.lastIndexOf('/')) + '/' + document.getElementById('typology').innerText.replace(/[^0-9]/g, ''), function callback(responseText) {
            // console.log("this.responseText;",responseText);
            window.responseText = responseText;
            setInitial();
        });
    };
    window.plusOneRoom = function () {
        if (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) + 1 < 5) {
            if (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) + 1 > 0) {
                document.getElementById('typology').innerText = (parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g, '')) + 1) + ' BHK';
            }
        } else {
            document.getElementById('typology').innerText = /*(parseInt(document.getElementById('typology').innerText.replace(/[^0-9]/g,''))+1) + */' VILLA';
        }
        let url = window.location.href;
        ajaxQueirer('/ajax/load-unit-plan/' + url.substring(url.lastIndexOf('/', url.lastIndexOf('/') - 1) + 1, url.lastIndexOf('/')) + '/' + document.getElementById('typology').innerText.replace(/[^0-9]/g, ''), function callback(responseText) {
            // console.log("this.responseText;",responseText);
            window.responseText = responseText;
            setInitial();
        });
    };
    window.ajaxQueirer = function (url, callback) {
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                callback(JSON.parse(this.responseText));
            }
        };
        xhttp.open("GET", url, true);
        xhttp.send();
    };
    window.setInitial = function () {
        var ptype = document.getElementById('ptype');
        jQuery('#ptype').empty();
        window.responseText.forEach(function (each) {
            jQuery('#ptype').append('<option value=' + each.field_room_type_name + '>' + each.field_room_type_name + '</option>');
        });
        if (window.responseText.length > 0) {
            jQuery('#price').text('₹' + window.responseText[0].field_unit_price / 100000 + " Lacs");
            jQuery('#specialPrice').text('Special Price: ₹' + window.responseText[0].field_specialprice / 100000 + " Lacs");
            jQuery('#saving').text('₹' + (window.responseText[0].field_unit_price - window.responseText[0].field_specialprice) / 100000 + " Lacs");
            jQuery('#planImage1').attr('src', 'http://' + window.responseText[0].field_unit_plan_images);
            jQuery('#planImage2').attr('src', 'http://' + window.responseText[0].field_unit_plan_images);
            jQuery('#possetion').text(mL[(new Date(window.responseText[0].possetion)).getMonth()] + ' ' + (new Date(window.responseText[0].possetion)).getFullYear());
        } else {
            jQuery('#price').text('');
            jQuery('#specialPrice').text('');
            jQuery('#saving').text('');
            jQuery('#planImage1').attr('src', '');
            jQuery('#planImage2').attr('src', '');
            jQuery('#possetion').text('');
        }
    };
    window.changePlan = function () {
        for (let a of window.responseText) {
            if (a.field_room_type_name === jQuery('#ptype option:selected').text()) {
                jQuery('#price').text('₹' + a.field_unit_price / 100000 + " Lacs");
                jQuery('#specialPrice').text('Special Price: ₹' + a.field_specialprice / 100000 + " Lacs");
                jQuery('#saving').text('₹' + (a.field_unit_price - a.field_specialprice) / 100000 + " Lacs");
                jQuery('#planImage1').attr('src', 'http://' + a.field_unit_plan_images);
                jQuery('#planImage2').attr('src', 'http://' + a.field_unit_plan_images);
                jQuery('#possetion').text(mL[(new Date(a.possetion)).getMonth()] + ' ' + (new Date(a.possetion)).getFullYear());
                break;
            }
        }
    }
    function reset() {
        minusOneRoom();
        plusOneRoom();
    }
</script>

